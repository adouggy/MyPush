<decorators defaultdir="/decorators">
	<excludes>
		<pattern>/includes/*</pattern>
		<pattern>/40*.jsp</pattern>
		<pattern>/dwr/*</pattern>
		<pattern>/resources/*</pattern>
		<pattern>/manage_no_decorator.do</pattern>
		<pattern>/application.wadl</pattern>
		<pattern>/*action=*</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">
		<pattern>/index.do</pattern>
		<pattern>/user.do</pattern>
		<pattern>/session.do</pattern>
		<pattern>/notification.do</pattern>
		<pattern>/manage.do</pattern>
	</decorator>
</decorators>
